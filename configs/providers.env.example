# CANIDAE Provider Credentials Configuration
# Copy this file to .env and fill in your actual credentials
# NEVER commit the actual .env file to version control

# Anthropic Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_ENDPOINT=https://api.anthropic.com/v1
ANTHROPIC_RATE_LIMIT=100

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_ENDPOINT=https://api.openai.com/v1
OPENAI_RATE_LIMIT=200

# Google/Gemini Configuration
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_ENDPOINT=https://generativelanguage.googleapis.com/v1
GOOGLE_RATE_LIMIT=150

# Together AI Configuration
TOGETHER_API_KEY=your-together-api-key-here
TOGETHER_ENDPOINT=https://api.together.xyz/v1
TOGETHER_RATE_LIMIT=300

# General Configuration
CANIDAE_LOG_LEVEL=info
CANIDAE_METRICS_ENABLED=true
CANIDAE_CHAOS_ENABLED=false
CANIDAE_FLOW_CONTROL_ENABLED=true